{
  "name": "praxigento/mobi_app_generic_mage2_work",
  "description": "Development/test environment for Magento v2 Generic MOBI application.",
  "type": "project",
  "homepage": "https://github.com/praxigento/mobi_app_generic_mage2",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Gusev",
      "email": "flancer64@gmail.com"
    }
  ],
  "require": {
    "php": "~5.5.0|~5.6.0|~7.0.0",
    "praxigento/composer_plugin_templates": "^1.0",
    "magento/magento-composer-installer": "^0.1",
    "magento/product-community-edition": "2.0.0",
    "praxigento/mobi_app_generic_mage2": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.0",
    "phpunit/php-code-coverage": "^2"
  },
  "autoload": {
    "psr-4": {
      "Magento\\Setup\\": "setup/src/Magento/Setup/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Praxigento\\Accounting\\Test\\Integration\\": "vendor/praxigento/mobi_mod_common_accounting/test/integration",
      "Praxigento\\Test\\": "test/fw/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "firegento": {
      "type": "composer",
      "url": "http://packages.firegento.com"
    },
    "magento": {
      "type": "composer",
      "url": "http://packages.magento.com/"
    },
    "praxigento/mobi_mod_common_accounting": {
      "type": "vcs",
      "url": "https://github.com/praxigento/mobi_mod_common_accounting"
