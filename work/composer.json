      "url": "https://github.com/praxigento/mobi_app_generic_mage2"
    }
  },
  "config": {
    "use-include-path": true
  },
  "scripts": {
    "pre-install-cmd": [
      "# Create root directory for the development Magento instance.",
      "mkdir -p htdocs"
    ],
    "post-install-cmd": [
      "# To complete installation launch 'sh ./bin/deploy/post_install.sh'"
    ]
  },
  "extra": {
    "magento-root-dir": "htdocs",
    "magento-deploystrategy": "copy",
    "magento-deploystrategy-overwrite": {
      "praxigento/mobi_app_generic_mage2": "symlink",
      "praxigento/mobi_mod_common_accounting": "symlink",
      "praxigento/mobi_mod_common_bonus_base": "symlink",
      "praxigento/mobi_mod_common_bonus_retail": "symlink",
      "praxigento/mobi_mod_common_core": "symlink",
      "praxigento/mobi_mod_common_dashboard": "symlink",
      "praxigento/mobi_mod_common_downline": "symlink",
      "praxigento/mobi_mod_common_mlm_id": "symlink",
      "praxigento/mobi_mod_common_pv": "symlink",
      "praxigento/mobi_mod_common_wallet": "symlink",
      "praxigento/mobi_mod_mage2_accounting": "symlink",
      "praxigento/mobi_mod_mage2_bonus_base": "symlink",
      "praxigento/mobi_mod_mage2_bonus_retail": "symlink",
      "praxigento/mobi_mod_mage2_core": "symlink",
      "praxigento/mobi_mod_mage2_dashboard": "symlink",
      "praxigento/mobi_mod_mage2_downline": "symlink",
      "praxigento/mobi_mod_mage2_mlm_id": "symlink",
      "praxigento/mobi_mod_mage2_pv": "symlink",
      "praxigento/mobi_mod_mage2_wallet": "symlink"
    },
    "magento-force": true,
    "auto-append-gitignore": false,
    "praxigento_templates_config": [
      "./templates.json",
      "./cfg/templates.json"
    ]
  }
