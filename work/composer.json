    "praxigento/mobi_mod_mage2_accounting": {
      "type": "vcs",
      "url": "https://github.com/praxigento/mobi_mod_mage2_accounting"
    },
    "praxigento/mobi_mod_mage2_bonus_base": {
      "type": "vcs",
      "url": "https://github.com/praxigento/mobi_mod_mage2_bonus_base"
